<Window x:Class="NeuronChatBot.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NeuronChatBot"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="363*"/>
            <RowDefinition Height="56*"/>
        </Grid.RowDefinitions>
        <TextBox Name="TextBoxInput" Grid.Row="0" Margin="10"></TextBox>
        <Button Grid.Row="1" Margin="10" Content="Добавить в БД" Name="ButtonAddDB" Click="ButtonAddDB_OnClick"></Button>
    </Grid>
</Window>
